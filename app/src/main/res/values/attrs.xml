<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HistoryView">
        <attr name="tag" format="string" />
        <attr name="value" format="integer" />
        <attr name="type" format="string" />
        <attr name="date" format="string" />
        <attr name="remark" format="enum">
            <enum name="income" value="0" />
            <enum name="pay" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="DetailView">
        <attr name="tT" format="integer" />
        <attr name="T" format="integer" />
        <attr name="fH" format="integer" />
        <attr name="tH" format="integer" />
        <attr name="H" format="integer" />
        <attr name="ft" format="integer" />
        <attr name="tt" format="integer" />
        <attr name="t" format="integer" />
        <attr name="f" format="integer" />
        <attr name="o" format="integer" />
    </declare-styleable>
    <declare-styleable name="PieChartView">
        <attr name="food" format="string|reference" />
        <attr name="food_value" format="fraction" />
        <attr name="trans" format="string|reference" />
        <attr name="trans_value" format="fraction" />
        <attr name="play" format="string|reference" />
        <attr name="play_value" format="fraction" />
        <attr name="usually" format="string|reference" />
        <attr name="usually_value" format="fraction" />
        <attr name="other" format="string|reference" />
    </declare-styleable>
    <declare-styleable name="PieView">
        <attr name="values" format="string" />
    </declare-styleable>
</resources>